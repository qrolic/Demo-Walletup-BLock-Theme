{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/banner-description-section",
  "version": "0.1.0",
  "title": "Banner Description Section",
  "category": "walletup",
  "icon": "  dashicons-admin-tools",
  "description": "this is an image, Heading, and description in banner-description-section",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "banner_description_section",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "Heading": {
      "type": "string",
      "source": "html",
      "selector": "h4"
    },
    "description": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "mediaID": {
      "type": "number"
    },
    "mediaURL": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    }
  }
}
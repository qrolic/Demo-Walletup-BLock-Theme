{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/zig-zag-section",
	"version": "0.1.0",
	"title": "Zig Zag Section",
	"category": "walletup",
	"icon": "  dashicons-welcome-learn-more",
	"description": "this is am image and description in zig-zag-section",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "zig-zag-section",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"Heading": {
		  "type": "string",
		  "source": "html",
		  "selector": "h2"
		},
		"mediaID": {
			"type": "number"
		},
		"mediaURL": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		},
		"buttonText":{
			"type": "string",
		  "source": "html",
		  "selector": "a"
		},
		"buttonURL": {
			"type": "string",
			"default": ""
		  }
	}
}

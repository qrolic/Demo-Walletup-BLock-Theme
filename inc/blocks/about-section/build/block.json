{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/about-page",
  "version": "0.1.0",
  "title": "About section",
  "category": "walletup",
  "icon": "  dashicons-superhero-alt",
  "description": "this is an image, text, and description in superhero-alt",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "about-page",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "Heading": {
      "type": "string",
      "source": "html",
      "selector": "h4"
    },
    "description": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "mediaID": {
      "type": "number"
    },
    "mediaURL": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    },
    "mediaID2": {
      "type": "number"
    },
    "mediaURL2": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    },
    "Heading_top": {
      "type": "string",
      "source": "html",
      "selector": "h2"
    }
  }
}